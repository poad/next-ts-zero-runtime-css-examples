{
  "name": "kuma-ui-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "pnpm dlx serve@latest out",
    "prettier:check": "prettier --check src",
    "eslint:check": "eslint --ext .ts,.js,.tsx,.jsx src",
    "prettier:fix": "prettier --write src",
    "eslint:fix": "eslint --fix --ext .ts,.js,.tsx,.jsx src",
    "lint": "pnpm run --parallel /.*:check/",
    "lint-fix": "pnpm run /.*:fix/",
    "clean": "rimraf build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@kuma-ui/core": "^1.5.5",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kuma-ui/next-plugin": "^1.1.15",
    "@kuma-ui/webpack-plugin": "^1.2.7",
    "@next/bundle-analyzer": "^14.0.4",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/nextjs": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "msw": "^2.0.13",
    "msw-storybook-addon": "^1.10.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.7",
    "typescript": "^5.3.3"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
