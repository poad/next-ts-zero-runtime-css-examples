{
  "name": "kuma-ui-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "pnpm dlx serve@latest out",
    "eslint:check": "eslint ./src/**/*{.ts,.tsx}",
    "eslint:fix": "eslint --fix ./src/**/*{.ts,.tsx}",
    "lint": "pnpm run --parallel /.*:check/",
    "lint-fix": "pnpm run /.*:fix/",
    "clean": "rimraf build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@kuma-ui/core": "^1.5.8",
    "next": "^14.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@kuma-ui/next-plugin": "^1.3.2",
    "@kuma-ui/webpack-plugin": "^1.4.2",
    "@next/bundle-analyzer": "^14.2.8",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-themes": "^8.2.9",
    "@storybook/blocks": "^8.2.9",
    "@storybook/nextjs": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "msw": "^2.4.2",
    "msw-storybook-addon": "^2.0.3",
    "rimraf": "^6.0.1",
    "storybook": "^8.2.9",
    "typescript": "^5.5.4"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
