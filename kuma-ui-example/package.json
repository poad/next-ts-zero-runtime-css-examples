{
  "name": "kuma-ui-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "pnpm dlx serve@latest out",
    "prettier:check": "prettier --check src",
    "eslint:check": "eslint ./src/**/*{.ts,.tsx}",
    "prettier:fix": "prettier --write src",
    "eslint:fix": "eslint --fix ./src/**/*{.ts,.tsx}",
    "lint": "pnpm run --parallel /.*:check/",
    "lint-fix": "pnpm run /.*:fix/",
    "clean": "rimraf build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@kuma-ui/core": "^1.5.7",
    "next": "^14.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@kuma-ui/next-plugin": "^1.3.1",
    "@kuma-ui/webpack-plugin": "^1.4.1",
    "@next/bundle-analyzer": "^14.2.4",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.1.11",
    "@storybook/nextjs": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "msw": "^2.3.1",
    "msw-storybook-addon": "^2.0.2",
    "rimraf": "^5.0.7",
    "storybook": "^8.1.11",
    "typescript": "^5.5.2"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
