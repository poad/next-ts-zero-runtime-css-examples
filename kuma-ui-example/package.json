{
  "name": "kuma-ui-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "pnpm dlx serve@latest out",
    "eslint-check": "eslint --ext .ts,.js,.tsx,.jsx src",
    "eslint-fix": "eslint --fix --ext .ts,.js,.tsx,.jsx src",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "lint": "pnpm prettier-check && pnpm eslint-check",
    "lint-fix": "pnpm prettier-format && pnpm eslint-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@kuma-ui/core": "^1.5.1",
    "next": "^13.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@kuma-ui/next-plugin": "^1.1.11",
    "@kuma-ui/webpack-plugin": "^1.2.3",
    "@next/bundle-analyzer": "^13.5.5",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.0",
    "@storybook/nextjs": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "msw": "^1.3.2",
    "msw-storybook-addon": "^1.9.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "storybook": "^7.5.0",
    "typescript": "^5.2.2"
  },
  "msw": {
    "workerDirectory": ".storybook/public"
  }
}
