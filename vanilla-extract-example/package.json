{
  "name": "vanilla-extract-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "pnpm dlx serve@latest out",
    "eslint-check": "eslint --ext .ts,.js,.tsx,.jsx src",
    "eslint-fix": "eslint --fix --ext .ts,.js,.tsx,.jsx src",
    "prettier-check": "prettier --check src",
    "prettier-format": "prettier --write src",
    "stylelint": "stylelint \"**/*.{js,jsx,ts,tsx}\"",
    "stylelint-fix": "stylelint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "pnpm prettier-check && pnpm eslint-check",
    "lint-fix": "pnpm prettier-format && pnpm eslint-fix",
    "clean": "rimraf build",
    "prelint": "pnpm install",
    "prebuild": "pnpm lint",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.5.4",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.4.6",
    "@storybook/nextjs": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vanilla-extract/next-plugin": "^2.3.1",
    "@vanilla-extract/webpack-plugin": "^2.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-next": "^13.5.4",
